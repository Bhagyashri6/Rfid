                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="Zebra FX7500 "
                            android:textStyle="bold"
                            android:textColor="#C3B399"
                            android:textSize="17sp" />
                    </LinearLayout>


                </FrameLayout>
            </LinearLayout>
            <View
